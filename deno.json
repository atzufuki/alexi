{
  "name": "@atzufuki/alexi",
  "version": "0.6.0",
  "exports": {
    ".": "./src/mod.ts",
    "./core": "./src/core/mod.ts",
    "./db": "./src/db/mod.ts",
    "./db/backends/denokv": "./src/db/backends/denokv/mod.ts",
    "./db/backends/indexeddb": "./src/db/backends/indexeddb/mod.ts",
    "./urls": "./src/urls/mod.ts",
    "./http": "./src/http/mod.ts",
    "./middleware": "./src/middleware/mod.ts",
    "./views": "./src/views/mod.ts",
    "./web": "./src/web/mod.ts",
    "./staticfiles": "./src/staticfiles/mod.ts",
    "./restframework": "./src/restframework/mod.ts",
    "./auth": "./src/auth/mod.ts",
    "./admin": "./src/admin/mod.ts",
    "./webui": "./src/webui/mod.ts",
    "./webui/launcher": "./src/webui/launcher.ts",
    "./webui/bindings": "./src/webui/bindings.ts",
    "./capacitor": "./src/capacitor/mod.ts",
    "./types": "./src/types/mod.ts"
  },
  "tasks": {
    "test": "deno test -A --unstable-kv --no-check",
    "check": "deno check src/**/mod.ts",
    "lint": "deno lint",
    "fmt": "deno fmt"
  },
  "imports": {
    "@alexi/types": "./src/types/mod.ts",
    "@alexi/core": "./src/core/mod.ts",
    "@alexi/db": "./src/db/mod.ts",
    "@alexi/db/backends/denokv": "./src/db/backends/denokv/mod.ts",
    "@alexi/db/backends/indexeddb": "./src/db/backends/indexeddb/mod.ts",
    "@alexi/urls": "./src/urls/mod.ts",
    "@alexi/http": "./src/http/mod.ts",
    "@alexi/middleware": "./src/middleware/mod.ts",
    "@alexi/views": "./src/views/mod.ts",
    "@alexi/web": "./src/web/mod.ts",
    "@alexi/staticfiles": "./src/staticfiles/mod.ts",
    "@alexi/restframework": "./src/restframework/mod.ts",
    "@alexi/auth": "./src/auth/mod.ts",
    "@alexi/admin": "./src/admin/mod.ts",
    "@alexi/webui": "./src/webui/mod.ts",
    "@alexi/webui/launcher": "./src/webui/launcher.ts",
    "@alexi/webui/bindings": "./src/webui/bindings.ts",
    "@alexi/capacitor": "./src/capacitor/mod.ts",
    "@std/assert": "jsr:@std/assert@1",
    "@std/path": "jsr:@std/path@1"
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any",
        "no-slow-types",
        "no-unused-vars",
        "require-await",
        "verbatim-module-syntax",
        "no-import-prefix",
        "constructor-super",
        "no-case-declarations",
        "no-empty",
        "no-inner-declarations",
        "no-this-alias",
        "no-unversioned-import",
        "no-var",
        "no-window",
        "no-window-prefix",
        "prefer-const"
      ]
    }
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 100,
    "singleQuote": false
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true
  }
}
